public class Classroom{
 	private HashMap<Point, Student> studentMap = new HashMap<>(); 
  	public Classroom()
    {
    
    }
  	public HashMap<Point,Student> getSeating()
    {
     	return studentMap; 
    }
  	public void setSeating(HashMap<Point, Student> studentMap)
    {
     	this.studentMap = studentMap; 
    }
  	public void seatStudent(Student student, Point p)
  	{
   		studentMap.put(p,student); 
  	}
  	public void getStudent(Point p)
    {
     	return studentMap.get(p); 
    }
  	public int getStudentCount()
    {
     	return studentMap.keySet().size(); 
    }
  
  	
}
